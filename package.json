{
    "name": "golf-card-game",
    "version": "1.0.0",
    "description": "Multiplayer Golf Card Game with real-time updates",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "dev": "tsx watch src/server.ts",
        "start": "node dist/server.js",
        "db:migrate": "node dist/db/migrate.js",
        "migrate:up": "node-pg-migrate up",
        "migrate:down": "node-pg-migrate down"
    },
    "keywords": [
        "golf",
        "card-game",
        "multiplayer",
        "socket.io"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "connect-pg-simple": "^9.0.1",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "pg": "^8.11.3",
        "socket.io": "^4.6.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.17.10",
        "@types/node": "^20.10.5",
        "@types/pg": "^8.10.9",
        "node-pg-migrate": "^8.0.3",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    }
}
